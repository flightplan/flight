{
  "name": "@flighthq/scene",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./DisplayObject": {
      "types": "./dist/DisplayObject.d.ts",
      "default": "./dist/DisplayObject.js"
    },
    "./DisplayObjectContainer": {
      "types": "./dist/DisplayObjectContainer.d.ts",
      "default": "./dist/DisplayObjectContainer.js"
    },
    "./Sprite": {
      "types": "./dist/Sprite.d.ts",
      "default": "./dist/Sprite.js"
    },
    "./internal/*": {
      "types": "./dist/internal/*.d.ts",
      "default": "./dist/internal/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "tsc -b --clean",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@flighthq/contracts": "^1.0.0",
    "@flighthq/math": "^1.0.0",
    "@flighthq/types": "^1.0.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  }
}
